<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="401.0" prefWidth="692.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.AuthorController">
   <children>
      <TableView fx:id="authorTable" layoutX="99.0" layoutY="118.0" prefHeight="283.0" prefWidth="467.0">
        <columns>
          <TableColumn fx:id="firstname" prefWidth="98.0" text="Firstname" />
            <TableColumn fx:id="lastname" minWidth="0.0" prefWidth="126.0" text="Lastname" />
        </columns>
      </TableView>
      <Button fx:id="refreshAuthors" layoutX="307.0" layoutY="31.0" mnemonicParsing="false" onAction="#onClickRefreshAuthors" text="Refresh" />
      <Button fx:id="addAuthor" layoutX="99.0" layoutY="72.0" mnemonicParsing="false" onAction="#onClickAddAuthor" text="Add author" />
   </children>
</AnchorPane>
